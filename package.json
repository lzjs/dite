{
  "name": "ditejs",
  "private": true,
  "description": "ditejs",
  "keywords": [
    "dite"
  ],
  "homepage": "https://github.com/ditejs/dite#readme",
  "bugs": {
    "url": "https://github.com/ditejs/dite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ditejs/dite.git"
  },
  "license": "MIT",
  "author": {
    "name": "Zack",
    "email": "lorenx@163.com",
    "url": "https://github.com/loyep"
  },
  "scripts": {
    "bootstrap": "dite-scripts bootstrap",
    "build": "dite-scripts turbo --cmd build",
    "build:deps": "dite-scripts turbo --cmd build:deps",
    "clean": "dite-scripts clean",
    "cov": "jest --coverage",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "esno scripts/cmd/postinstall.ts",
    "jest": "jest",
    "lint": "prettier --write .",
    "prepare": "husky install",
    "release": "dite-scripts release",
    "test": "jest"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@dite/bundler-utils": "workspace:*",
    "@dite/bundler-vite": "workspace:*",
    "@dite/bundler-webpack": "workspace:*",
    "@dite/cli": "workspace:*",
    "@dite/core": "workspace:*",
    "@dite/node": "workspace:*",
    "@dite/preset-dite": "workspace:*",
    "@dite/react": "workspace:*",
    "@dite/server": "workspace:*",
    "@dite/utils": "workspace:*",
    "@jest/types": "^28.1.1",
    "@pnpm/lockfile-file": "^5.1.4",
    "@pnpm/logger": "^4.0.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/resolve": "^1.20.2",
    "@vercel/ncc": "^0.34.0",
    "cpy": "^9.0.1",
    "create-dite": "workspace:*",
    "dite": "workspace:*",
    "dite-scripts": "workspace:*",
    "dts-packer": "^0.0.3",
    "esbuild": "^0.14.48",
    "esno": "^0.16.3",
    "fs-extra": "^10.0.1",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lerna": "4.0.0",
    "lint-staged": "^13.0.3",
    "only-allow": "^1.1.1",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolve": "^1.22.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "tsup": "^6.1.2",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4",
    "uglify-js": "^3.16.2",
    "webpack": "^5.73.0",
    "zx": "4.3.0"
  },
  "packageManager": "pnpm@7.3.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "postcss",
        "webpack",
        "eslint",
        "stylelint"
      ]
    }
  }
}
