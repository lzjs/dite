{
  "name": "dite-monorepo",
  "private": true,
  "homepage": "https://dite.dev/",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "dite-scripts bootstrap",
    "build": "dite-scripts turbo --cmd build",
    "build:client": "umi-scripts turbo --cmd build:client",
    "build:deps": "pnpm --parallel --filter \"./packages/*\" build:deps",
    "build:force": "umi-scripts turbo --cmd build --no-cache",
    "clean": "dite-scripts clean",
    "cov": "jest --coverage",
    "dep:update": "pnpm up --interactive --latest --recursive",
    "format": "prettier --cache --write .",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "dite-scripts postinstall",
    "jest": "jest",
    "lint": "dite-scripts lint",
    "prepare": "husky install",
    "release": "dite-scripts release",
    "test": "jest",
    "tsc:check": "tsc --noEmit",
    "version": "pnpm install && git add pnpm-lock.yaml"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@dite/bundler-utils": "workspace:*",
    "@dite/bundler-vite": "workspace:*",
    "@dite/bundler-webpack": "workspace:*",
    "@dite/cli": "workspace:*",
    "@dite/core": "workspace:*",
    "@dite/nest": "workspace:*",
    "@dite/node": "workspace:*",
    "@dite/preset-dite": "workspace:*",
    "@dite/react": "workspace:*",
    "@dite/utils": "workspace:*",
    "@jest/types": "^28.1.1",
    "@pnpm/lockfile-file": "^5.1.4",
    "@pnpm/logger": "^4.0.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/resolve": "^1.20.2",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vercel/ncc": "^0.34.0",
    "cpy": "^9.0.1",
    "create-dite": "workspace:*",
    "dite": "workspace:*",
    "dite-scripts": "workspace:*",
    "dts-packer": "^0.0.3",
    "esbuild": "^0.14.48",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.0.1",
    "git-repo-info": "^2.1.1",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lerna": "^5.1.8",
    "lint-staged": "^13.0.3",
    "only-allow": "^1.1.1",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolve": "^1.22.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "tsup": "^6.1.2",
    "tsx": "^3.8.0",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4",
    "uglify-js": "^3.16.2",
    "webpack": "^5.73.0",
    "zx": "4.3.0"
  },
  "packageManager": "pnpm@7.3.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "postcss",
        "webpack",
        "eslint",
        "stylelint"
      ]
    }
  }
}
